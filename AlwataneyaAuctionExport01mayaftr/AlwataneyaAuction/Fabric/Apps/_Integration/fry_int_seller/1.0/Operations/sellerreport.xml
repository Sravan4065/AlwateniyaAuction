        <config-param api-version="" appid="" name="entitymetadata" value="{&quot;dml&quot;:false,&quot;query&quot;:&quot;WITH HighestBids AS (\n    SELECT \n        object_id, \n        MAX(bid_amount) AS max_bid_amount\n    FROM auction_bidding \n    GROUP BY object_id\n),\nFilteredBids AS (\n    SELECT \n        ab.object_id,\n        ab.bid_amount,\n        ab.is_approved\n    FROM auction_bidding ab\n    JOIN HighestBids hb ON ab.object_id = hb.object_id AND ab.bid_amount = hb.max_bid_amount\n)\n\nSELECT \n    u.name, \n    u.reg_date, \n    COUNT(DISTINCT o.object_id) AS total_objects, \n    COUNT(DISTINCT a.auction_id) AS auction_vehicles,\n    COUNT(DISTINCT CASE WHEN a.auction_id IS NULL THEN o.object_id END) AS unauctioned_vehicles,\n    SUM(a.total_commission) AS total_commission,\n    COUNT(DISTINCT CASE WHEN fb.is_approved = 0 THEN o.object_id END) AS highest_bid_not_approved_vehicles,\n    SUM(CASE WHEN ab.is_approved = 1 THEN ab.bid_amount ELSE 0 END) AS total_approved_bid_amount\nFROM users u\nLEFT JOIN asset_objects o ON o.created_by = u.user_id\nLEFT JOIN auctions a ON a.object_id = o.object_id\nLEFT JOIN auction_bidding ab ON o.object_id = ab.object_id\nLEFT JOIN FilteredBids fb ON o.object_id = fb.object_id -- Only highest bids considered\nWHERE u.is_seller = 1\nGROUP BY u.name, u.reg_date;&quot;,&quot;overloadPosition&quot;:0,&quot;serviceInput&quot;:[{&quot;type&quot;:&quot;STRING&quot;,&quot;name&quot;:&quot;foundrySerivceName&quot;,&quot;inoutType&quot;:0,&quot;sqlDataType&quot;:0,&quot;ordinalPosition&quot;:0,&quot;paramClass&quot;:&quot;com.kony.rdbms.metadata.model.ParamTypeData&quot;},{&quot;type&quot;:&quot;STRING&quot;,&quot;name&quot;:&quot;serviceAction&quot;,&quot;inoutType&quot;:0,&quot;sqlDataType&quot;:0,&quot;ordinalPosition&quot;:0,&quot;paramClass&quot;:&quot;com.kony.rdbms.metadata.model.ParamTypeData&quot;}],&quot;serviceOutput&quot;:[],&quot;name&quot;:&quot;CustomQuery&quot;,&quot;schemaName&quot;:&quot;dbo&quot;,&quot;serviceType&quot;:&quot;com.kony.rdbms.metadata.model.CustomQueryMetadata&quot;}"/>
